{
  "preset": "laravel",
  "rules": {
    "global_namespace_import": {
      "import_classes": true,
      "import_constants": true,
      "import_functions": true
    },
    "ordered_imports": {
      "sort_algorithm": "length",
      "imports_order": [
        "const",
        "class",
        "function"
      ]
    },
    "ordered_interfaces": true,
    "ordered_traits": true,
    "single_line_empty_body": true
  }
}